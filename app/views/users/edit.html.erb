<!-- 회원가입 기능 start -->
	<div class="container">
		<h3>회원정보 수정</h3>
		<form action="/users/edit_complete" method="post" enctype="multipart/form-data">
			<div>
				Email : <input type="text" name="email" placeholder="<%= @user_ed.email %>">	
			</div>
			<div>
				Password : <input type="password" name="password">
			</div>
			<div>
				Password Confirm : <input type="password" name="re_password">
			</div>
			<div>
				Phone : <input type="text" name="phone" placeholder="<%= @user_ed.phone %>">
			</div>
			<div>
				Nickname : <input type="text" name="nickname" placeholder="<%= @user_ed.nickname %>">
			</div>
			<div>
				<input type="file" name="profile_pic" placeholder="<%= @user_ed.profile_pic_url(:thumb) %>">
			</div>
			<div>
				<input type="submit" value="수정하기" class="btn btn-danger btn-md">
			</div>
	</div>
<!-- 회원가입 기능 end -->

